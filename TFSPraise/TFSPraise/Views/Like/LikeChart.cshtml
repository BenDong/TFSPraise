<script type="text/javascript" async>
    $(function () {
        var options = {
            exportEnabled: true,
            animationEnabled: true,
            axisX: {
                gridThickness: 0
            },
            axisY: {
                gridThickness: 0
            },
            data: [
            {
                type: "splineArea", 
                dataPoints: @Html.Raw(ViewBag.DataPoints),
            }
            ]
        };

        $("#flot-dashboard-chart").CanvasJSChart(options);

    });

</script>